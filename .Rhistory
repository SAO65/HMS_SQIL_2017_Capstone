USA.DELTA_HALE <- (df.ROW.2010$hale[df.ROW.2010$country == ID.ROW.2] - df.ROW.1990$hale[df.ROW.1990$country == ID.ROW.2]) / (df.ROW.1990$hale[df.ROW.1990$country == ID.ROW.2]) * 100
USA.DELTA_YLD <- (df.ROW.2010$yld[df.ROW.2010$country == ID.ROW.2] - df.ROW.1990$yld[df.ROW.1990$country == ID.ROW.2]) / (df.ROW.1990$yld[df.ROW.1990$country == ID.ROW.2])* 100
USA.DELTA_THE <- (df.ROW.2010$the[df.ROW.2010$country == ID.ROW.2] - df.ROW.1990$the[df.ROW.1990$country == ID.ROW.2]) / (df.ROW.1990$the[df.ROW.1990$country == ID.ROW.2])* 100
USA.DELTA_GDP <- (df.ROW.2010$gdp[df.ROW.2010$country == ID.ROW.2] - df.ROW.1990$gdp[df.ROW.1990$country == ID.ROW.2]) / (df.ROW.1990$gdp[df.ROW.1990$country == ID.ROW.2])* 100
USA_QUALITYe <- USA.DELTA_HALE / USA.DELTA_YLD
USA_COSTe <- USA.DELTA_THE / USA.DELTA_GDP
USA_VALUEe <- USA_QUALITYe /  USA_COSTe
df.sust.USA <- data.frame(DELTA_HALE=USA.DELTA_HALE,DELTA_YLD= USA.DELTA_YLD, QUALITYe=USA_QUALITYe, DELTA_THE=USA.DELTA_THE,DELTA_GDP=USA.DELTA_GDP, COSTe=USA_COSTe, VALUEe=USA_VALUEe)
