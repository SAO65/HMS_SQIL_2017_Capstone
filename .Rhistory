DT.WHO.GDP$X1990 != "NA" &
DT.WHO.GDP$X2010 != "NA"
, ]
data.WHO.GDP
df.WHO.GDP <- data.frame(country=df.WHO.GDP$Country.Code, the.gdp=round(df.WHO.GDP$X1990, 0))
df.WHO.GDP
df.merge.1 <- merge(df.GBD, df.WHO.THE, all.x=F, by="country")
df.merge.1
intermediate.data <- merge(df.merge.1, df.WHO.THE.GDP, all.x=F, all.y=FALSE, by="country")
intermediate.data
# Years Lived with Disability and GDP
yld <- intermediate.data$le - intermediate.data$hale
gdp <- round(intermediate.data$the / intermediate.data$the.gdp, 0) * 100
# Tidy Dataset
tidy.data <- data.frame(intermediate.data, yld, gdp)
tidy.data
attach(tidy.data)
# Saving Tidy Dataset to .csv file
file_name <- paste("Output_EU_", output_year, ".csv", sep="" )
file_name
write.csv(tidy.data, file_name)
