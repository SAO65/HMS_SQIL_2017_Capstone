df.sust <- data.frame(Country=df.EU.2010$country)
df.sust$DELTA_HALE <- round((df.EU.2010$hale - df.EU.1990$hale) / df.EU.1990$le, 3) * 100
df.sust$DELTA_YLD <- round((df.EU.2010$yld - df.EU.1990$yld) / df.EU.1990$le, 3) * 100
df.sust$DELTA_THE <- round((df.EU.2010$the - df.EU.1990$the) / df.EU.1990$the, 3) * 100
df.sust$HALEe <- round(df.sust$DELTA_HALE / df.sust$DELTA_THE, 3) * 100
df.sust$YLDe <- round(df.sust$DELTA_YLD / df.sust$DELTA_THE, 3) * 100
df.sust$VALUEe <- round(df.sust$HALEe + df.sust$YLDe, 3)
print(df.sust, row.names = F)
