ex.axis=0.7, tick=-.01)\n\naxis(4, at=japan.the.2010, col.axis=\"black\", col=\"black\",las=2, cex.axis=0.7, tick=-.01)\n\npoints(df.ROW.2010$hale[df.ROW.2010$country == ID.ROW.1], df.ROW.2010$the[df.ROW.2010$country == ID.ROW.1], col=\"black\", pch = 8, cex = 1.3, lty = \"solid\", lwd = 2)\n\nabline(h=df.ROW.2010$the[df.ROW.2010$country == ID.ROW.1], v=df.ROW.2010$hale[df.ROW.2010$country == ID.ROW.1], lty=1, lwd=1)\n\n\nfun.EU.1990 <- lm(df.EU.1990$the ~ df.EU.1990$hale)\nabline(fun.EU.1990, lty = 2, col=\"blue\", lwd=1.5)\n\nfun.EU.2010 <- lm(df.EU.2010$the ~ df.EU.2010$hale)\nabline(fun.EU.2010, lty = 2, col=\"red\", lwd=1.5)\n\nsummary(fun.EU.1990)\nsummary(fun.EU.2010)\n\nconfint(fun.EU.1990)\nconfint(fun.EU.2010)\n\n```\n\n```{r}\npar(mar=c(5, 4, 4, 5) + 0.1)\n\nplot(df.EU.1990$yld, df.EU.1990$the, xlab=\"Years Lived with Disease\", ylab=\"Per Capita Total Health Expenditure\", col=\"blue\", pch = 16, cex = 1.3, lty = \"solid\", lwd = 2, xlim=lim.yld, ylim=lim.the, axes=FALSE, frame.plot=FALSE)\n\npoints(df.EU.2010$yld, df.EU.2010$the,  col=\"red\", pch = 16, cex = 1.3, lty = \"solid\", lwd = 2)\n\npoints(df.ROW.1990$yld[df.ROW.1990$country == ID.ROW.2], df.ROW.1990$the[df.ROW.1990$country == ID.ROW.2],  col=\"black\", pch = 5, cex = 1.3, lty = \"solid\", lwd = 2)\n\npoints(df.ROW.2010$yld[df.ROW.2010$country == ID.ROW.2], df.ROW.2010$the[df.ROW.2010$country == ID.ROW.2],  col=\"black\", pch = 9, cex = 1.3, lty = \"solid\", lwd = 2)\n\nlegend(8, 7000, c(\"EU 1990\",\"EU 2010\",\"USA 1990\", \"USA 2010\", \"JPN 2010\"), col=c(\"blue\", \"red\", \"black\", \"black\", \"black\"), bty=\"n\", y.intersp=1.5, pch=c(16,16, 5, 9, 8), cex = 1)\n\naxis(1, at=c(round(min(df.EU.1990$yld), 1), round(median(df.EU.1990$yld), 1), round(max(df.EU.1990$yld), 1)), col.axis=\"blue\", col=\"blue\", las=2, cex.axis=0.7,  tick=-.01)\n\naxis(3, at=round(df.ROW.2010$yld[df.ROW.2010$country == ID.ROW.1], 2), col.axis=\"black\", col=\"black\",las=2, cex.axis=0.7, tick=-.01)\n\n\nlow.the.1990 <- round(min(df.EU.1990$the), 0)\nmedian.the.1990 <- round(median(df.EU.1990$the), 0)\nhigh.the.1990 <- round(max(df.EU.1990$the), 0)\n\naxis(2, at=c(low.the.1990,median.the.1990, high.the.1990), col.axis=\"blue\", col=\"blue\",las=2, cex.axis=0.7, tick=-.01)\n\naxis(3, at=c(round(min(df.EU.2010$yld), 1), round(median(df.EU.2010$yld), 1), round(max(df.EU.2010$yld), 1)), col.axis=\"red\", col=\"red\", las=2, cex.axis=0.7, tick=-.01)\n\nlow.the.2010 <- round(min(df.EU.2010$the), 0)\nmedian.the.2010 <- round(median(df.EU.2010$the), 0)\nhigh.the.2010 <- round(max(df.EU.2010$the), 0)\njapan.the.2010 <- round(df.ROW.2010$the[df.ROW.2010$country == ID.ROW.1], 1)\n\naxis(4, at=c(low.the.2010,median.the.2010, high.the.2010), col.axis=\"red\", col=\"red\",las=2, cex.axis=0.7, tick=-.01)\n\naxis(4, at=japan.the.2010, col.axis=\"black\", col=\"black\",las=2, cex.axis=0.7, tick=-.01)\n\npoints(df.ROW.2010$yld[df.ROW.2010$country == ID.ROW.1], df.ROW.2010$the[df.ROW.2010$country == ID.ROW.1], col=\"black\", pch = 8, cex = 1.3, lty = \"solid\", lwd = 2)\n\nabline(h=df.ROW.2010$the[df.ROW.2010$country == ID.ROW.1], v=df.ROW.2010$yld[df.ROW.2010$country == ID.ROW.1], lty=1, lwd=1)\n\n\nfun.EU.1990 <- lm(df.EU.1990$the ~ df.EU.1990$yld)\nabline(fun.EU.1990, lty = 2, col=\"blue\", lwd=1.5)\n\nfun.EU.2010 <- lm(df.EU.2010$the ~ df.EU.2010$yld)\nabline(fun.EU.2010, lty = 2, col=\"red\", lwd=1.5)\n\nsummary(fun.EU.1990)\nsummary(fun.EU.2010)\n\nconfint(fun.EU.1990)\nconfint(fun.EU.2010)\n\n```\n\n\n# Sustainable Quality EU\n\n```{r}\ndf.sust.EU <- data.frame(Country=df.EU.2010$country)\n\ndf.sust.EU$DELTA_HALE <- round((df.EU.2010$hale - df.EU.1990$hale) / df.EU.1990$hale, 3) * 100\n\ndf.sust.EU$DELTA_YLD <- round((df.EU.2010$yld - df.EU.1990$yld) / df.EU.1990$yld, 3) * 100\n\ndf.sust.EU$QUALITYe <- round(df.sust.EU$DELTA_HALE / df.sust.EU$DELTA_YLD, 3)\n\n\ndf.sust.EU$DELTA_THE <- round((df.EU.2010$the - df.EU.1990$the) / df.EU.1990$the, 3)  * 100\n\ndf.sust.EU$DELTA_GDP <- round((df.EU.2010$gdp - df.EU.1990$gdp) / df.EU.1990$gdp, 3)  * 100\n\ndf.sust.EU$COSTe <- round(df.sust.EU$DELTA_THE / df.sust.EU$DELTA_GDP, 3)\n\ndf.sust.EU$VALUEe <- round(df.sust.EU$QUALITYe / df.sust.EU$COSTe, 3)\n\nprint(df.sust.EU, row.names = F)\n\n(ID.EU.Value <- df.sust.EU$Country[df.sust.EU$VALUEe == max(df.sust.EU$VALUEe)])\n\nID <- \"USA\"\n# ID <- \"JPN\"\n\nDELTA_HALE.ID <- (df.ROW.2010$hale[df.ROW.2010$country == ID] - df.ROW.1990$hale[df.ROW.1990$country == ID]) / (df.ROW.1990$hale[df.ROW.1990$country == ID]) * 100\n\nDELTA_YLD.ID <- (df.ROW.2010$yld[df.ROW.2010$country == ID] - df.ROW.1990$yld[df.ROW.1990$country == ID]) / (df.ROW.1990$yld[df.ROW.1990$country == ID])* 100\n\nDELTA_THE.ID <- (df.ROW.2010$the[df.ROW.2010$country == ID] - df.ROW.1990$the[df.ROW.1990$country == ID]) / (df.ROW.1990$the[df.ROW.1990$country == ID])* 100\n\nDELTA_GDP.ID <- (df.ROW.2010$gdp[df.ROW.2010$country == ID] - df.ROW.1990$gdp[df.ROW.1990$country == ID]) / (df.ROW.1990$gdp[df.ROW.1990$country == ID])* 100\n\nQUALITYe.ID <- DELTA_HALE.ID / DELTA_YLD.ID\n\nCOSTe.ID <- DELTA_THE.ID / DELTA_GDP.ID\n\nVALUEe.ID <- QUALITYe.ID /  COSTe.ID\n\ndf.sust.ID <- data.frame(DELTA_HALE.ID,DELTA_YLD.ID, QUALITYe.ID, DELTA_THE.ID,DELTA_GDP.ID, COSTe.ID, VALUEe.ID)\n\ndf.sust.ID\n\n```\n\n```{r}\n\nX <- df.sust.EU$VALUEe\n\nhist(X, prob=TRUE, col=\"skyblue\", border=\"black\", las=1, axes=F, breaks=5, xlim=c(0,1), ylim=c(0,3))\naxis(1)\naxis(2)\nlines(density(X), col=\"blue\", lwd=2) \nlines(density(X, adjust=2), lty=\"dotted\", col=\"black\", lwd=2)\nabline(v=mean(X), col=\"red\", lwd=1)\nabline(v=median(X), col=\"black\", lty=1, lwd=1)\nlegend(.5, .5, c(paste(\"mean=\", round(mean(X),2)),paste(\"median=\", round(median(X),2))), col=c(\"red\", \"black\"), bty=\"n\", y.intersp=1.5, lty=c(1,1), cex = 1, lwd=1)\nsummary(X)\n\n\nmu=mean(df.sust.EU$VALUEe); var=var(df.sust.EU$VALUEe)\nestBetaParams <- function(mu, var) {\n  alpha <- ((1 - mu) / var - 1 / mu) * mu ^ 2\n  beta <- alpha * (1 / mu - 1)\n  return(params = list(alpha = alpha, beta = beta))\n}\nestBetaParams(mu, var)\n\ny=function (x) dbeta(x,estBetaParams(mu, var)$alpha\n,estBetaParams(mu, var)$beta)\ncurve(y, from=0, to=1.5, col=\"blue\", axes=F, lwd=2, xlab=\"VALUEe\", ylab = \"\")\naxis(1)\naxis(1, at=round(qbeta(.025,estBetaParams(mu, var)$alpha,estBetaParams(mu, var)$beta), 2))\naxis(1, at=round(qbeta(.975,estBetaParams(mu, var)$alpha,estBetaParams(mu, var)$beta), 2))\nlines(density(X), col=\"skyblue\", lwd=2) \nabline(v=qbeta(.025,estBetaParams(mu, var)$alpha,estBetaParams(mu, var)$beta), lty=3)\nabline(v=qbeta(.975,estBetaParams(mu, var)$alpha,estBetaParams(mu, var)$beta), lty=3)\nabline(v=mean(df.sust.EU$VALUEe), col=\"black\", lty=4)\nabline(v=df.sust.ID$VALUEe.ID, col=\"red\")\nlegend(.8, 2.5, c(\"Density\",\"Beta Distribution\",paste(\"EU mean=\", round(mean(X),2)),paste(\"EU 95% CI=\", round(qbeta(.025,estBetaParams(mu, var)$alpha,estBetaParams(mu, var)$beta),2),\"-\", round(qbeta(.975,estBetaParams(mu, var)$alpha,estBetaParams(mu, var)$beta),2)), paste(ID,\"=\", round(df.sust.ID$VALUEe.ID, 2))), col=c(\"skyblue\", \"blue\", \"blue\", \"black\", \"red\"), bty=\"n\", y.intersp=1.5, lty=c(1,1,4,3, 1), cex = 1, lwd=1)\n\n```\n\n\n# Write files to excel for export to ppt\n```{r}\nrequire(xlsx)\nwrite.xlsx(df.EU.1990, \"df.EU.xlsx\", sheetName=\"1990\")\nwrite.xlsx(df.EU.2010, \"df.EU.xlsx\", sheetName=\"2010\", append=TRUE)\nwrite.xlsx(df.sust.EU, \"df.EU.xlsx\", sheetName=\"DELTA-EPSILON\", append=TRUE)\n\n```\n\n",
