, col=\"skyblue\", pch = 16, cex = 1.3, lty = \"solid\", lwd = 2, xlim=c(30, 90), ylim=c(4, 15), axes=FALSE, frame.plot=FALSE)\n\npoints(df.EU.1990$le, df.EU.1990$yld, col=\"skyblue\", pch = 16, cex = 1.3, lty = \"solid\", lwd = 2)\n\npoints(df.ROW.2010$le, df.ROW.2010$yld, col=\"blue\", pch = 16, cex = 1.3, lty = \"solid\", lwd = 2)\n\npoints(df.EU.2010$le, df.EU.2010$yld,  col=\"skyblue\", pch = 16, cex = 1.3, lty = \"solid\", lwd = 2)\n\naxis(1, col.axis=\"black\", las=2, cex.axis=0.7, tick=-.01)\n\naxis(2, at=(round(min(df.ROW.1990$yld), 0): round(max(df.ROW.1990$yld), 0)), col.axis=\"skyblue\", las=2, cex.axis=0.7, tick=-.01)\n\naxis(4, at=(round(min(df.ROW.2010$yld), 0): round(max(df.ROW.2010$yld), 0)), col.axis=\"blue\", las=2, cex.axis=0.7, tick=-.01)\n\ntext(40, 12, \"minmax 1990\", col=\"skyblue\")\ntext(40, 11, \"minmax 2010\", col=\"blue\")\n\npoints(df.ROW.2010$le[df.ROW.2010$country == ID.ROW.1], df.ROW.2010$yld[df.ROW.2010$country == ID.ROW.1], pch=21, col=\"black\", cex = 1.3, lty = \"solid\", lwd = 2)\n\ntext(df.ROW.2010$le[df.ROW.2010$country == ID.ROW.1], df.ROW.2010$yld[df.ROW.2010$country == ID.ROW.1], labels=paste(ID.ROW.1, \"2010\"), cex= .8, pos=1, offset = .5)\n\nabline(h=df.ROW.2010$yld[df.ROW.2010$country == ID.ROW.1], v=df.ROW.2010$le[df.ROW.2010$country == ID.ROW.1], lty=2)\n\nID.min = df.ROW.2010$country[df.ROW.2010$yld.2010 == min(df.ROW.2010$yld.2010)]\nID.min\n\nID.max = df.ROW.2010$country[df.ROW.2010$yld.2010 == max(df.ROW.2010$yld.2010)]\nID.max\n\ntext(df.ROW.2010$le.2010[df.ROW.2010$country == ID.min], df.ROW.2010$yld.2010[df.ROW.2010$country == ID.min], labels=ID.min, cex= .8, pos=4, offset = .5)\n\ntext(df.ROW.2010$le.2010[df.ROW.2010$country == ID.max], df.ROW.2010$yld.2010[df.ROW.2010$country == ID.max], labels=ID.max, cex= .8, pos=4, offset = .5)\n\n```\n\n\n# Expansion of Morbidity EU-ROW: 1990\n\n```{r}\npar(mar=c(5, 4, 4, 5) + 0.1)\n\nplot(df.ROW.1990$le, df.ROW.1990$yld, main=\"Global Burden of Disease Study 2010 (GBDx)