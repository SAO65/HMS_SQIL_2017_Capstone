ylim=NULL, axes=TRUE, frame.plot=FALSE)\npoints( df.ROW$the.2010, QUALITY.ROW.2010, col=\"black\", pch = 16, cex = 1.3, lty = \"solid\", lwd = 2)\npoints(df.EU$the.1990, QUALITY.EU.1990, col=\"blue\", pch = 16, cex = 1.3, lty = \"solid\", lwd = 2)\npoints( df.EU$the.2010, QUALITY.EU.2010, col=\"red\", pch = 16, cex = 1.3, lty = \"solid\", lwd = 2)\n\nfun.EU.2010 <- lm( QUALITY.EU.2010 ~ df.EU.2010$the )\nfun.EU.1990 <- lm( QUALITY.EU.1990 ~ df.EU.1990$the)\n\nabline(fun.EU.1990, lty = 3, col=\"blue\", lwd=1.5)\nabline(fun.EU.2010, lty = 3, col=\"red\", lwd=1.5)\n\n\n```\n\n\n\n# Value EU - ROW: 1990-2010\n\n```{r}\n\npar(mfrow=c(2,2))\n\nVALUE.EU.1990 <-  QUALITY.EU.1990 / df.EU$the.1990 * 100\nVALUE.ROW.1990 <-  QUALITY.ROW.1990 / df.ROW$the.1990 * 100\n\nVALUE.EU.2010 <-  QUALITY.EU.2010 / df.EU$the.2010* 100\nVALUE.ROW.2010 <-  QUALITY.ROW.2010 / df.ROW$the.2010* 100\n\nplot(VALUE.ROW.1990, VALUE.ROW.2010, main=\"Value \\nROW 1990-2010\", xlab=\"VALUE 1990\", ylab=\"VALUE 2010\", col=\"skyblue\",  pch = 16, cex = 1.3, lwd = 2, lty = \"solid\", axes=TRUE, frame.plot=FALSE, xlim=c(0, 30), ylim=c(0, 30))\n\npoints( VALUE.EU.1990, VALUE.EU.2010, main=\"Value \\nEU 1990-2010\", xlab=\"YEAR 1990 -- Int USD PPP\", ylab=\"YEAR 2010 -- Int USD PPP\", col=\"red\", pch = 16, cex = 1.3, lty = \"solid\", lwd = 2)\n\n\nplot(VALUE.ROW.1990, VALUE.ROW.2010, main=\"Value \\nROW 1990-2010\", xlab=\"VALUE 1990\", ylab=\"VALUE 2010\", col=\"skyblue\",  pch = 16, cex = 1.3, lwd = 2, lty = \"solid\", axes=TRUE, frame.plot=FALSE, xlim=c(0, 15), ylim=c(0, 15))\n\npoints( VALUE.EU.1990, VALUE.EU.2010, main=\"Value \\nEU 1990-2010\", xlab=\"YEAR 1990 -- Int USD PPP\", ylab=\"YEAR 2010 -- Int USD PPP\", col=\"red\", pch = 16, cex = 1.3, lty = \"solid\", lwd = 2)\n\n\nplot(VALUE.ROW.1990, VALUE.ROW.2010, main=\"Value \\nROW 1990-2010\", xlab=\"VALUE 1990\", ylab=\"VALUE 2010\", col=\"skyblue\",  pch = 16, cex = 1.3, lwd = 2, lty = \"solid\", axes=TRUE, frame.plot=FALSE, xlim=c(0, 7.5), ylim=c(0, 7.5))\n\npoints( VALUE.EU.1990, VALUE.EU.2010, main=\"Value \\nEU 1990-2010\", xlab=\"YEAR 1990 -- Int USD PPP\", ylab=\"YEAR 2010 -- Int USD PPP\", col=\"red\", pch = 16, cex = 1.3, lty = \"solid\", lwd = 2)\n\n\nplot(VALUE.ROW.1990, VALUE.ROW.2010, main=\"Value \\nROW 1990-2010\", xlab=\"VALUE 1990\", ylab=\"VALUE 2010\", col=\"skyblue\",  pch = 16, cex = 1.3, lwd = 2, lty = \"solid\", axes=TRUE, frame.plot=FALSE, xlim=c(0, 3.75), ylim=c(0, 3.75))\n\npoints( VALUE.EU.1990, VALUE.EU.2010, main=\"Value \\nEU 1990-2010\", xlab=\"YEAR 1990 -- Int USD PPP\", ylab=\"YEAR 2010 -- Int USD PPP\", col=\"red\", pch = 16, cex = 1.3, lty = \"solid\", lwd = 2)\n\n\n```\n\n\n# Growth of Total Health Expenditure WORLD: 1990-2010\n\n```{r}\npar(mfrow=c(1,1))\npar(mar=c(5, 4, 4, 5) + 0.1)\n\nplot(df.ROW.1990$yld, df.ROW.1990$the, main=\"Growth of Total Health Expenditure \\nWorld 1990-2010\", xlab=\"Years Lived with Disease\", ylab=\"Per Capita Total Health Expenditure\", col=\"grey\",  pch = 16, cex = 1.3, lwd = 2, lty = \"solid\", xlim=c(4,15), ylim=c(0, 10000), axes=TRUE, frame.plot=FALSE)\n\npoints(df.ROW.2010$yld, df.ROW.2010$the, col=\"black\", pch = 16, cex = 1.3, lty = \"solid\", lwd = 2)\n\npoints(df.EU.1990$yld, df.EU.1990$the, col=\"blue\", pch = 16, cex = 1.3, lty = \"solid\", lwd = 2)\n\npoints(df.EU.2010$yld, df.EU.2010$the,  col=\"red\", pch = 16, cex = 1.3, lty = \"solid\", lwd = 2)\n\n\nfun.EU.2010 <- lm(df.EU.2010$the ~ df.EU.2010$yld)\nfun.EU.1990 <- lm(df.EU.1990$the ~ df.EU.1990$yld)\n\nabline(fun.EU.1990, lty = 3, col=\"blue\", lwd=1.5)\nabline(fun.EU.2010, lty = 3, col=\"red\", lwd=1.5)\n\n\n\npoints(df.ROW.2010$yld[df.ROW.2010$country == ID.ROW.1], df.ROW.2010$the[df.ROW.2010$country == ID.ROW.1], pch=21, col=\"black\", cex = 1.3, lty = \"solid\", lwd = 2)\n\ntext(df.ROW.2010$yld[df.ROW.2010$country == ID.ROW.1], df.ROW.2010$the[df.ROW.2010$country == ID.ROW.1], labels=paste(ID.ROW.1, \"2010\"), cex= .8, pos=2, offset = .5)\n\nabline(h=df.ROW.2010$the[